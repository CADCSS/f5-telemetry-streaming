{
    "class": "Telemetry",
    "My_Poller": {
        "class": "Telemetry_System_Poller",
        "enabled": true,
        "trace": false,
        "interval": 300,
        "host": "x.x.x.x",
        "port": 443,
        "username": "myuser",
        "passphrase": {
            "environmentVar": "MY_ENV_VAR"
        }
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "enabled": true,
        "trace": false,
        "port": 6514
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "enabled": true,
        "trace": false,
        "type": "Azure_Log_Analytics",
        "host": "myworkspaceid",
        "passphrase": {
            "environmentVar": "MY_CONSUMER_ENV_VAR"
        }
    }
 }
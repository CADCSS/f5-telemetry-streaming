{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "host": "192.0.2.1",
        "port": 443,
        "username": "myuser",
        "passphrase": {
            "environmentVar": "MY_SYSTEM_ENV_VAR"
        },
        "systemPoller": "My_Poller",
        "iHealthPoller": "My_iHealth"
    },
    "My_Poller": {
        "class": "Telemetry_System_Poller",
        "interval": 60
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    },
    "My_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Azure_Log_Analytics",
        "workspaceId": "myworkspaceid",
        "passphrase": {
            "environmentVar": "MY_CONSUMER_ENV_VAR"
        }
    },
    "My_iHealth": {
        "class": "Telemetry_iHealth_Poller",
        "username": "username",
        "passphrase": {
            "environmentVar": "MY_IHEALTH_ENV_VAR"
        },
        "interval": {
            "timeWindow": {
                "start": "00:00",
                "end": "05:00"
            }
        }
    }
 }
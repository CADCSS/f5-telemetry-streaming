{
    "class": "Telemetry",
    "My_System": {
        "class": "Telemetry_System",
        "systemPoller": {
            "interval": 60,
            "tag": {
                "tenant": "`T`",
                "application": "`A`",
                "foo": "bar"
            }
        }
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514,
        "tag": {
            "tenant": "`T`",
            "application": "`A`",
            "foo": "bar"
        }
    }
}
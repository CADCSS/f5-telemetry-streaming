    "repository": {
        "type": "git",
        "url": "git+https://github.com/F5Networks/f5-telemetry-streaming.git"
    },
    "scripts": {
        "install-production": "npm install --production --no-optional",
        "install-test": "npm install --no-optional",
        "lint": "eslint src test scripts",
        "test-functional": "mocha --recursive \"./test/functional/*.js\" --opts ./test/functional/.mocha.opts",
        "test-only": "mocha --recursive \"./test/unittests/*.js\" --opts ./test/unittests/.mocha.opts",
        "test": "nyc --reporter html --reporter text --check-coverage --lines 80 --functions 80 --branches 70 npm run test-only"
    },
    "dependencies": {
        "ajv": "^6.5.4",
        "ajv-async": "^1.0.1",
        "aws-sdk": "^2.369.0",
        "commander": "^2.19.0",
        "deep-diff": "^1.0.2",
        "elasticsearch": "^15.3.0",
        "kafka-node": "^2.6.1",
        "mustache": "^3.0.0",
        "node-statsd": "0.1.1",
        "request": "^2.88.0"
    },
    "devDependencies": {
        "eslint": "^5.4.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.16.0",
        "icrdk": "git://github.com/f5devcentral/f5-icontrollx-dev-kit#master",
        "mocha": "^5.2.0",
        "nyc": "^13.3.0",
        "ssh2": "^0.8.2"
    }
}

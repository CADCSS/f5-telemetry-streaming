{
    "stats": {
        "version": {
            "key": "/mgmt/tm/sys/version::sys/version/0::Version",
            "normalize": true
        },
        "versionSequence": {
            "key": "sequenceFromBash::commandResult"
        },
        "configReady": {
            "key": "/mgmt/tm/sys/ready::sys/ready/0::configReady"
        },
        "licenseReady": {
            "key": "/mgmt/tm/sys/ready::sys/ready/0::licenseReady"
        },
        "provisionReady": {
            "key": "/mgmt/tm/sys/ready::sys/ready/0::provisionReady"
        },
        "syncStatus": {
            "key": "/mgmt/tm/cm/sync-status::cm/sync-status/0"
        },
        "failoverStatus": {
            "key": "/mgmt/tm/cm/failover-status::cm/failover-status/0"
        },
        "cpu": {
            "key": "/mgmt/tm/sys/host-info::sys/host-info/0::sys/hostInfo/0/cpuInfo",
            "filterByKeys": [ "/cpuInfo", "fiveSecAvgRatio", "oneMinAvgUser", "oneMinAvgSystem" ],
            "renameKeysByPattern": { "/cpuInfo": { "pattern": "cpuInfo\/(.*)", "group": 1 } }
        },
        "memory": {
            "key": "/mgmt/tm/sys/memory::sys/memory/memory-host::sys/memory/memory-host/0",
            "filterByKeys": [ "memoryTotal", "memoryFree", "tmmMemoryTotal", "tmmMemoryFree" ]
        },
        "provisionState": {
            "key": "/mgmt/tm/sys/provision::items",
            "convertArrayToMap": { "keyName": "name", "keyNamePrefix": "module/" },
            "filterByKeys": [ "module/", "level" ],
            "renameKeysByPattern": { "module/": { "pattern": "module\/(.*)", "group": 1 } }
        },
        "networkInterfaces": {
            "key": "/mgmt/tm/net/interface/stats",
            "filterByKeys": [ "/stats", "counters.bitsIn", "counters.bitsOut" ],
            "renameKeysByPattern": { "net/interface": "([0-9]\\.[0-9]|(?!\/)mgmt(?=\/))" }
        },
        "tmmCpu": {
            "key": "/mgmt/tm/sys/tmm-info",
            "filterByKeys": [ "/tmm-info", "oneMinAvgUsageRatio" ],
            "renameKeysByPattern": { "sys/tmm-info": "([0-9]\\.[0-9])" }
        },
        "tmmTraffic": {
            "key": "/mgmt/tm/sys/tmm-traffic",
            "filterByKeys": [ "/tmm-traffic", "clientSideTraffic.bitsIn", "clientSideTraffic.bitsOut", "clientSideTraffic.pktsIn", "clientSideTraffic.pktsOut" ],
            "renameKeysByPattern": { "sys/tmm-traffic": "([0-9]\\.[0-9])" }
        },
        "virtualServerStats": {
            "key": "/mgmt/tm/ltm/virtual/stats",
            "filterByKeys": [ "/stats", "clientside.bitsIn", "clientside.bitsOut", "clientside.pktsIn", "clientside.pktsOut", "clientside.curConns", "availabilityState",  "enabledState", "destination" ],
            "renameKeysByPattern": { "ltm/virtual": { "pattern": "virtual\/(.*)\/", "group": 1 } }
        }
    }
}
        "ltmPolicies": {
            "key": "ltmPolicies",
            "filterKeys": { "exclude": [ "policyName", "vsName", "action", "actionId", "ruleName", "tmTarget" ] },
            "renameKeys": { "patterns": [ { "ltm/policy/": { "pattern": "policy\/(.*)\/", "group": 1 }, "/actions/": { "pattern": "actions\/(.*)", "group": 1 } }, { "/actions": { "constant": "actions" } } ] },
            "addKeysByTag": true
